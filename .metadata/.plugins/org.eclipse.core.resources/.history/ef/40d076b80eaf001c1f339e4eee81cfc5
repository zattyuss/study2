package test;

import java.util.ArrayList;
import java.util.List;

import model.Member;

public class Test3 {
	public static void main(String[] args) {
		Member member1 = new Member(1, "홍길동1", "51241", "hong1@naver.com");
		Member member3 = new Member(3, "홍길동3", "55241", "hong3@naver.com");
		List<Member> memberList = new ArrayList<Member>();
		memberList.add(member1);
		memberList.add(new Member(2, "홍길동2", "2135", "hong2@naver.com"));
		memberList.add(member3);
		memberList.add(new Member(4, "홍길동4", "24135", "hong4@naver.com"));

		
		
		System.out.println(memberList);
		int size = memberList.size();
		int lastIndex = memberList.size()-1;
		System.out.println("memberList의 길이 : " + size);
		System.out.println("memberList의 마지막인덱스 : " + lastIndex);
		
		
		Member getMember1 = memberList.get(3);
		int id = getMember1.getId();
		String name = getMember1.getName();		
		System.out.println(getMember1);		
		System.out.println("회원번호 : " + id);		
		System.out.println("회원이름 : " + name);
		
		
	}
}
