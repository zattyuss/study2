package ex04;

public class PotionDemo {



}

interface Potion{
	void recovery();
}

class HealPotion implements Potion{

	@Override
	public void recovery() {
		System.out.println("힐포션");
	}
	
}

class ManaPotion implements Potion{

	@Override
	public void recovery() {
		System.out.println("마나포션");
	}
	
}